{% extends 'base.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('assets/css/warehouse/structure.css') }}">
{% endblock %}

{% block body %}
    <div class="container-fluid mt-5">
        {{ include('warehouse/warehouse_structure/_grid_display.html.twig') }}
        {{ render(controller('App\\Warehouse\\Controller\\WarehouseStructureController::new')) }}
    </div>

    <input type="hidden" id="warehouse-structure-open-endpoint" value="{{ path('app_warehouse_structure_open', {id: 'fileId'}) }}">
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script type="module" src="{{ asset('assets/js/warehouse/warehouse_structure.js') }}"></script>
{% endblock %}